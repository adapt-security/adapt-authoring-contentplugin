  "description": "Module for managing framework plugins",
  "homepage": "https://github.com/adapt-security/adapt-authoring-contentplugin",
  "main": "index.js",
  "type": "module",
  "dependencies": {
    "adapt-cli": "github:adaptlearning/adapt-cli#v3.3.3",
